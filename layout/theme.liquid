    }
  </style>
  <script>
    document.addEventListener("DOMContentLoaded", function () {
      function replaceUnavailableText() {
        document.querySelectorAll(".sold_out").forEach(element => {
          if (element.textContent.trim().toLowerCase() === "unavailable") {
            element.textContent = "Sold Out";
            element.classList.add("badge-sold-out"); // Add styling class
          }
        });
      }
      // Run on page load
      replaceUnavailableText();

      // Run again if the DOM changes (for dynamically loaded content)
      const observer = new MutationObserver(replaceUnavailableText);
      observer.observe(document.body, { childList: true, subtree: true });
    });
  </script>
